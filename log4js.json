{
    "appenders": {
      "console": {
        "type": "console"
      },
      "file": {
        "type": "file",
        "filename": "logs/application.log",
        "maxLogSize": 10485760,
        "backups": 3,
        "compress": true
      }
    },
    "categories": {
      "default": {
        "appenders": [
          "console",
          "file"
        ],
        "level": "info"
      }
    }
  }
  